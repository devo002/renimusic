                <div class="container">
                  <div class="row justify-content-lg-start shift">
                    <div class="col-lg-8">
                      <% slide2.forEach(quote => { %>
                        <h5 data-caption-animate="fadeInUp" data-caption-delay="100" style="color: white; font-family: cursive; text-transform: none; font-size: 25px;"><%= quote.text %></h5>
                        <h1 data-caption-animate="fadeInUp" data-caption-delay="200" style="font-size: 20px; color: white; font-family: cursive; text-transform: none;"><%= quote.verse %></h1>
